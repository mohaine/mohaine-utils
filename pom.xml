<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.sonatype.oss</groupId>
        <artifactId>oss-parent</artifactId>
        <version>7</version>
    </parent>


    <groupId>com.mohaine</groupId>
    <artifactId>mohaine-utils</artifactId>
    <packaging>jar</packaging>
    <version>1.0.0_SNAPSHOT</version>
    <description>Collection of simple utils</description>
    <url>http://maven.apache.org</url>
    <licenses>
        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>repo</distribution>
        </license>
    </licenses>
    <scm>
        <url>https://github.com/mohaine/mohaine-utils.git/tags/mohaine-utils-1.0.0</url>
        <connection>https://github.com/mohaine/mohaine-utils.git/tags/mohaine-utils-1.0.0</connection>
    </scm>
    <developers>
        <developer>
            <id>mohaine</id>
            <name>Michael Graessle</name>
            <email>mike-pom@graessle.net</email>
        </developer>
    </developers>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
        </configuration>
       </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-gpg-plugin</artifactId>
        <version>1.5</version>
        <configuration>
          <keyname>mike@graessle.net</keyname>
        </configuration>
      </plugin>

    </plugins>
  </build>


</project>
